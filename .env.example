# Configuração para Deploy no Vercel
# Copie este arquivo para .env.local no Vercel

# Database
DATABASE_URL=postgresql://username:password@hostname:5432/database_name

# Session Secret (gere uma string aleatória)
SESSION_SECRET=your-super-secret-session-key-here

# Environment
NODE_ENV=production

# Opcional: Para debugging
DEBUG=false

# Exemplo de URLs de PostgreSQL para diferentes providers:
# Neon: postgresql://username:password@ep-xxx.us-east-1.aws.neon.tech/neondb
# Supabase: postgresql://postgres:password@db.xxx.supabase.co:5432/postgres
# Railway: postgresql://postgres:password@containers-us-west-xxx.railway.app:7432/railway